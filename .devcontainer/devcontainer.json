{
  "name": "RevProxyTest",
  "dockerComposeFile": "compose.yml",
  "service": "dev",
  "workspaceFolder": "/workspaces/NginxRevProxySidecarTest",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {},
    "ghcr.io/devcontainers/features/desktop-lite:1": {
      "password": "noPassword",
      "webPort": "6901",
      "vncPort": "5901"
    }
  },
  "postCreateCommand": "./.devcontainer/postCreate.bash",
  "postAttachCommand": "./.devcontainer/postAttach.bash",
  "remoteEnv": {
    "DOCKER_API_VERSION": "1.43"
  },
  "forwardPorts": [6901, "farmos:80", "proxy:443"],
  "portsAttributes": {
    "6901": {
      "label": "noVNC",
      "protocol": "http",
      "onAutoForward": "silent"
    },
    "farmos:80": {
      "label": "http",
      "protocol": "http",
      "onAutoForward": "silent"
    },
    "proxy:443": {
      "label": "https",
      "protocol": "https",
      "onAutoForward": "silent"
    }
  },
  // "otherPortsAttributes": {
  //   "label": "farmOS",
  //   "protocol": "https",
  //   "onAutoForward": "silent"
  // },
  "customizations": {
    "vscode": {
      "settings": {
        "remote.autoForwardPorts": true,
        "remote.autoForwardPortsSource": "hybrid"
      }
    }
  }
}